{
    "name": "williamtrevisan/ddd-patterns-php",
    "description": "Application with PHP to study ddd patterns",
    "authors": [
        {
            "name": "William Trevisan",
            "email": "williamtrev@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": [
                "tests/Feature",
                "tests/Unit"
            ]
        }
