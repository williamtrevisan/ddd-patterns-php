    "require": {
        "ramsey/uuid": "^4.3",
        "doctrine/orm": "^2.12",
        "symfony/cache": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "laravel/pint": "^0.1.6"
    }
}
